 body
	margin 0
	padding 0
.all
	.header
		position fixed
		width 100%
		background rgb(97,66,30)
		height 60px
		z-index 999
		.nav
			margin 0 auto
			padding 10px 0
			width 1145px
			zoom 1
			&::after
				display block
				clear both
				content ""
				width 0
				height 0
				visibility hidden
			img
				float left
				vertical-align middle
				margin-right 40px
			div
				float left
				display inline-block
			.intro
				padding-top 8px
				a
					position relative
					float left
					margin-right 5px
					padding 6px 15px
					font-size 14px
					color white
					i
						position absolute
						display block
						background white
						&.t
							transition width .5s linear
							height 1px
							width 0px
							top 0
							left 0
						&.l
							transition height .5s linear
							width 1px
							height 0px
							top 0
							left 0
						&.b
							transition width .5s linear
							height 1px
							width 0px
							bottom 0
							right 0
						&.r
							transition height .5s linear
							width 1px
							height 0px
							top 0
							right 0
					&:hover .t
						width 100%
					&:hover .l
						height 100%
					&:hover .b
						width 100%
					&:hover .r
						height 100%
			.align
				padding-top 12px
				.hot-line
					font-size 14px
					color white
				.telphone
					font-family "宋体"
					font-size 18px
					color rgb(187,187,187)
	.leftLink
		position fixed
		right -120px
		top 200px
		width 160px
		height 278px
		cursor pointer
		background url(../../pro/img/suspend.png)
		z-index 999
		.online
			float left
			width 40px
			height 278px
		.contant
			float right
			width 120px
			height 278px
			.relation
				float left
				width 120px
				height 92.6px
	.banner
		position relative
		width 100%
		height @width
		a
			position absolute
			top 0
			left 0
			z-index 2
			img
				width 100%
				height @width
		.arrow
			position absolute
			width 38px
			height 61px
			top 300px
			cursor pointer
			opacity 0.5
			z-index 2
		.left
			display none
			left 0
			background url(../../pro/img/themes.gif)
		.right
			display none
			right 0
			background url(../../pro/img/themes.gif) -38px 0px
		&:hover .left
			display block
		&:hover .right
			display block
		.icon
			position absolute
			z-index 3
			bottom 0
			left 50%
			margin-left -37px
	.main-part-1
		position relative
		width 100%
		height 480px
		.container
			position relative
			width 90%
			height 480px
			margin 0 auto
			.img
				position absolute
				width 40%
				top 200px
				z-index 1
				img
					width 100%
			.text
				position absolute
				right 0px
				top 120px
				text-align right
				img
					padding-bottom 30px
				p
					color rgb(129,129,129)
					line-height 30px
			.slideAnimation
				animation move 0.8s linear
	.main-part-2
		position relative
		width 100%
		height 751px
		background black
		z-index 1
		.pic
			position absolute
			z-index 2
			overflow hidden
			width 100%
			height 375.5px
			cursor pointer
			.shadow
				position absolute
				z-index 3
				width 100%
				height 375.5px
				background-color rgb(0,0,0)
				opacity 0.5
				filter unquote("alpha(opacity=40)")
				// animation changed 0.2s linear infinite
				&:hover
					opacity 0
					filter unquote("alpha(opacity=0)")
			
			.addShadow
				animation changed 0.2s linear
			.img
				overflow hidden
				position absolute
				width 380px
				height 80px
				top 160px
		.first
			position absolute
			z-index 2
			width 100%
			height 375.5px
			.big
				position absolute
				width 100%
				height 375.5px
				left -30px
				z-index 2
			.img
				position absolute
				right 200px
				z-index 4
				.firstPic
					width 377px
					height 56px
					position absolute
				.addRight
					animation moveRight 0.5s linear
		.second
			position absolute
			z-index 2
			top 375.5px
			width 100%
			height 375.5px
			.big
				position absolute
				width 100%
				height 375.5px
				left -30px
				z-index 2
			.img
				position absolute
				left 200px
				z-index 4
				.secondPic
					position absolute
					width 377px
					height 56px
				.addLeft
					animation moveLeft 0.5s linear
	.main-part-3
		position relative
		width 100%
		height 900px
		padding-top 80px
		background url(../../pro/img/img_sec5.jpg)
		background-size 100% 
		.title
			position relative
			overflow hidden
			margin 0 auto
			width 276px
			height 105px
			.partTitle
				position absolute
			.addTop
				animation moveTop 0.5s cubic-bezier(0.155, 0.330, 0.205, 0.895)
		.bottom
			margin 150px auto
			width 1084px
			.common
				display inline-block
				overflow hidden
				border 1px solid white
				width 255px
				height 344px
				text-align center
				color white
				line-height 24px
				span
					display block
				.chdesc
					font-size 22px
					letter-spacing 4px
				.endesc
					letter-spacing 2px
					word-spacing 2px
					font-size 14px
				.top
					overflow hidden
					margin 40px auto
					width 100px
					height 100px
				.more
					margin-top 40px
				.part2
					position relative
					width 255px
					top -450px
					color rgb(96,50,12)
					text-align left
					.desc
					.more2
					.top2
						position absolute
						left 50%
					.desc
						width 200px
						margin-left -100px
						text-align center
					.top2
						margin-left -50px
					.more2
						width 70px
						margin-left -35px
						text-align center
			.ab
				margin-left 18px
			.advantage
				position relative
				.top1
					background url(../../pro/img/20160517160159573acff759548.png)
				.top2
					background url(../../pro/img/20160517160159573acff759548.png) -2px 100px
			.policy
				position relative
				.top1
					background url(../../pro/img/20160517160218573ad00aaa139.png)
				.top2
					background url(../../pro/img/20160517160218573ad00aaa139.png) -2px 93px
			.mode
				position relative
				.top1
					background url(../../pro/img/20160517160230573ad016d650a.png)
				.top2
					background url(../../pro/img/20160517160230573ad016d650a.png) -2px 110px
			.condition
				position relative
				.top1
					background url(../../pro/img/20160517160242573ad022756b6.png)
				.top2
					background url(../../pro/img/20160517160242573ad022756b6.png) -2px 104px
	.main-part-4
		position relative
		width 100%
		height 200px
		overflow hidden
		.animate
			position absolute
			width 100%
		.addRight
			animation 0.5s right linear
			.item
				position relative
				overflow hidden
				width 24.95%
				float left
				cursor pointer
				z-index 2
				&>img
					position relative
					z-index 3
					width 100%
					height @width
				.shadow
					position absolute
					z-index 4
					top 0
					width 100%
					height 99%
					left 0
					background-color rgb(0,0,0)
					opacity 0.5
					filter unquote("alpha(opacity=50)")
					&:hover
						opacity 0
						filter unquote("alpha(opacity=0)")
				.addShadow
					animation changed 0.2s linear
				.text
					position absolute
					z-index 5
					top 50%
					left 5%
					width 80%
					height 20%
					overflow hidden
					.title
						position absolute
						width 60%
						height @width
						left 30%
					.addUp
						animation 0.5s moveUp linear
	.main-part-5
		width 100%
		overflow hidden
		padding 60px 0
		overflow hidden
		.container
			margin 0 auto
			width 80%
			height 10%
			.img
				position relative
				margin 0 auto
				float left
				width 24%
				height 30%
				border 1px solid transparent
				a
					position absolute
					float left
					width 20%
					height 30%
					margin-left 4%
					border 1px solid transparent
					span
						font-size 20px
						padding-left 106px
						color rgb(162,134,134)
					.bg
						position absolute
						display block
						top -30px
						width 106px
						height 78px
						border 1px solid transparent
				.job
					.bg
						background url(../../pro/img/ico_common.png) 0px -111px
				.video
					.bg
						background url(../../pro/img/ico_common.png) -94px -111px
				.download
					.bg
						background url(../../pro/img/ico_common.png) -188px -111px
				.faq
					.bg
						background url(../../pro/img/ico_common.png) -282px -111px
				.addMoveUp1
					animation moveUp1 0.3s cubic-bezier(0.155, 0.330, 0.205, 0.895)
				.addMoveUp2
					animation moveUp2 0.6s cubic-bezier(0.155, 0.330, 0.205, 0.895)
				.addMoveUp3
					animation moveUp3 0.9s cubic-bezier(0.155, 0.330, 0.205, 0.895)
				.addMoveUp4
					animation moveUp4 1.2s cubic-bezier(0.155, 0.330, 0.205, 0.895)
	.footer
		width 100%
		background-color rgb(76,50,35)
		padding 30px 0 30px 0
		overflow hidden
		font-size 14px
		.cover
			margin 0 auto
			overflow hidden
			width 1240px
			.link
				color rgb(186,186,186)
			.container
				padding-bottom 20px
				margin-bottom 20px
				overflow hidden
				border-bottom 1px solid white
				.part
					overflow hidden
					.img
						float left
				.firstPart
					float left
					width 35%
					margin-top 15px
					a
						padding 5px 10px
						margin-right 2px
						margin-top 8px
						border 1px solid rgb(113,113,113)
						color rgb(160,160,160)
						float left
						white-space nowrap
						&:hover
							border 1px solid white
							color white
				.secondPart
					padding-left 20px
					float left
					margin-top 23px
					width 25%
					color rgb(186,186,186)
					line-height 25px
					&>div
						overflow hidden
						margin-bottom 10px
						.img
							width 26px
							height 26px
						.imgMail
							background url(../../pro/img/ico_common.png) -124px -45px
						.imgQQ
							background url(../../pro/img/ico_common.png) -69px -42px
						.imgXin
							background url(../../pro/img/ico_common.png) -245px -48px
						a
							white-space nowrap
							margin-left 5px
							width 100%
				.thirdPart
					width 35%
					float right
					margin 5px
					line-height 20px
					.title
						color rgb(186,186,186)
					.normal
						color rgb(206,206,206)
			.bottom
				width 100%
				margin 0 auto
				text-align center
				color rgb(206,206,206)
				.police
					color rgb(206,206,206)
				.partner
					color white
@keyframes move
	from
		opacity 0
		right -300px
	to
		opacity 1
		right 0
@keyframes moveRight
	from
		left -379px
	to
		left 0	
@keyframes moveLeft
	from
		left 379px
	to
		left 0		
@keyframes changed
	from
		opacity 0
	to
		opacity 0.5
@keyframes moveTop
	from
		top 110px
	to
		top 0
@keyframes moveUp
	from
		top 48px
	to
		top 0
@keyframes right
	from
		left -100px
	to
		left 0
@keyframes moveUp1
	from
		opacity 0
		top 100px
	to
		opacity 1
		top 0
@keyframes moveUp2
	from
		opacity 0
		top 200px
	to
		opacity 1
		top 0
@keyframes moveUp3
	from
		opacity 0
		top 300px
	to
		opacity 1
		top 0
@keyframes moveUp4
	from
		opacity 0
		top 400px
	to
		opacity 1
		top 0
